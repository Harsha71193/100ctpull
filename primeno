public class primeNo
{
  public static void main(String[] args)
  {
     int a=27;
     int count=0;
     
     
     for(int i=2;i<a;i++)
     {     
           if(a%i==0)
           {
                count++;
           }
      }
      
      if(count==0)
      {
       System.out.println("No is prime");
      }
      else
      {
      System.out.println("no is not prime");
      }
  }
}
